<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">
    <!--
        Added the constraints for entity Movie.
    -->
    <changeSet id="20230102012454-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="movie_id"
                                 baseTableName="rel_movie__right"
                                 constraintName="fk_rel_movie__right__movie_id"
                                 referencedColumnNames="id"
                                 referencedTableName="movie"/>

        <addForeignKeyConstraint baseColumnNames="right_id"
                                 baseTableName="rel_movie__right"
                                 constraintName="fk_rel_movie__right__right_id"
                                 referencedColumnNames="id"
                                 referencedTableName="game_round"/>

        <addForeignKeyConstraint baseColumnNames="movie_id"
                                 baseTableName="rel_movie__left"
                                 constraintName="fk_rel_movie__left__movie_id"
                                 referencedColumnNames="id"
                                 referencedTableName="movie"/>

        <addForeignKeyConstraint baseColumnNames="left_id"
                                 baseTableName="rel_movie__left"
                                 constraintName="fk_rel_movie__left__left_id"
                                 referencedColumnNames="id"
                                 referencedTableName="game_round"/>
    </changeSet>
</databaseChangeLog>
