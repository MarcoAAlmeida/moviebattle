
entity GameSession {
    userId String
    finished Boolean
}

entity GameRound {
    userChoice Choice
    correct Boolean
}

entity Movie {
	imdbId String
    title String
    imdbRating Double
    imdbVotes Long
    score Double
}

enum Choice {
    RIGHT, LEFT, NONE
}

relationship OneToMany {
	GameSession to GameRound{gameSessionId}

}

relationship OneToMany {
  Movie{rightRounds} to GameRound{left}
  Movie{leftRounds} to GameRound{right}
}

service all with serviceImpl

