
entity GameSession {
    userId Long
}

entity GameRound {
    userChoice Choice
    correct Boolean
}

entity Movie {
	imdbId String
    title String
    imdbRating Double
    imdbVotes Long
    score Double
}

enum Choice {
    RIGHT, LEFT
}

relationship OneToMany {
	GameSession to GameRound{gameSessionId}

}

relationship ManyToMany {
	Movie to GameRound{rightId}
	Movie to GameRound{leftId}
}

service all with serviceImpl

